    <contexts>
    <!-- different formats:
      @tag                     : TagOnly
      @tag rest_of_line        : TagString
      @tag <word>              : TagWord
      @tag <word> <word>       : TagWordWord
      @tag <word> rest_of_line : TagWordString
    -->
      <context attribute="Normal Text" lineEndContext="#stay" name="Normal">
        <StringDetect attribute="Comment" context="LineComment" String="///"/>
        <StringDetect attribute="Comment" context="LineComment" String="//!"/>
        <StringDetect attribute="Comment" context="BlockComment" String="/**" beginRegion="Comment"/>
        <StringDetect attribute="Comment" context="BlockComment" String="/*!" beginRegion="Comment"/>
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="LineComment">
        <keyword attribute="Tags" context="#pop" String="TagOnly" />
        <keyword attribute="Tags" context="TagWord" String="TagWord" />
        <keyword attribute="Tags" context="TagWordWord" String="TagWordWord" />
        <keyword attribute="Tags" context="TagString" String="TagString" />
        <keyword attribute="Tags" context="TagWordString" String="TagWordString" />
      </context>
      <context attribute="Comment" lineEndContext="#stay" name="BlockComment">
        <keyword attribute="Tags" context="#stay" String="TagOnly" />
        <keyword attribute="Tags" context="TagWord" String="TagWord" />
        <keyword attribute="Tags" context="TagWordWord" String="TagWordWord" />
        <keyword attribute="Tags" context="TagString" String="TagString" />
        <keyword attribute="Tags" context="TagWordString" String="TagWordString" />
        <Detect2Chars attribute="Comment" context="#pop" char="*" char1="/" endRegion="BlockComment"/>
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="TagWord">
        <Detect2Chars attribute="Comment" context="#pop#pop" char="*" char1="/" endRegion="BlockComment"/>
        <RegExpr attribute="TagWord" context="#pop" String="\S\s" />
        <RegExpr attribute="TagWord" context="#stay" String="\s*\S" />
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="TagWordWord">
        <Detect2Chars attribute="Comment" context="#pop#pop" char="*" char1="/" endRegion="BlockComment"/>
        <RegExpr attribute="TagWord" context="Tag2ndWord" String="\S\s" />
        <RegExpr attribute="TagWord" context="#stay" String="\s*\S" />
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="Tag2ndWord">
        <Detect2Chars attribute="Comment" context="#pop#pop#pop" char="*" char1="/" endRegion="BlockComment"/>
        <RegExpr attribute="TagWordWord" context="#pop#pop" String="\S\s" />
        <RegExpr attribute="TagWordWord" context="#stay" String="\s*\S" />
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="TagString">
        <RegExpr attribute="Comment" context="#stay" String="\s*"/>
        <Detect2Chars attribute="Comment" context="#pop#pop" char="*" char1="/" endRegion="BlockComment" />
        <RegExpr attribute="TagString" context="#stay" String="." />
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="TagWordString">
        <Detect2Chars attribute="Comment" context="#pop#pop" char="*" char1="/" endRegion="BlockComment"/>
        <RegExpr attribute="TagWord" context="#pop" String="\S\s" />
        <RegExpr attribute="TagWord" context="#stay" String="\s*\S" />
      </context>
    </contexts>
    <itemDatas>
      <itemData name="Normal Text" defStyleNum="dsNormal" />
      <itemData name="Tags"        defStyleNum="dsKeyword" color="#ca60ca" selColor="#ffffff" bold="1" italic="0" />
      <itemData name="TagWord"     defStyleNum="dsKeyword" color="#0095ff" selColor="#ffffff" bold="1" italic="0" />
      <itemData name="TagWordWord" defStyleNum="dsKeyword" color="#0095ff" selColor="#ffffff" bold="1" italic="0" />
      <itemData name="TagString"   defStyleNum="dsString"  color="#ff0000" />
      <itemData name="Comment"     defStyleNum="dsComment" color="#0000ff" selColor="#ffffff" italic="1" />
    </itemDatas>
  </highlighting>
