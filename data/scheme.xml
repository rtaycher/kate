    <contexts>
      <context name="Intro" attribute="Error" lineEndContext="#stay">
        <DetectChar attribute="Comment" context="Comment" char=";"/>
        <Detect2Chars attribute="Comment" context="MultiLineComment" char="|" char1="#"/>
        <DetectChar attribute="Operator" context="Normal" char="("/>
      </context>
      <context name="Normal" attribute="Normal" lineEndContext="#stay">
        <DetectChar attribute="Comment" context="Comment" char=";"/>
        <Detect2Chars attribute="Comment" context="MultiLineComment" char="|" char1="#"/>
        <DetectChar attribute="Operator" context="Normal" char="("/>
        <DetectChar attribute="Operator" context="#pop" char=")"/>
        <keyword attribute="String" context="#stay" String="blocks"/>
        <keyword attribute="Keyword" context="#stay" String="keywords"/>
        <keyword attribute="Keyword" context="Predicates" String="predicates"/>
        <keyword attribute="Keyword" context="Predicates2" String="predicates2"/>
        <keyword attribute="Keyword" context="Assignments" String="assignments"/>
        <keyword attribute="MathOps" context="#stay" String="mathops"/>
        <RegExpr attribute="MathOps" context="#stay" String="[\+\-\*\/]?\s"/>
        <StringDetect attribute="Special" context="#stay" String="list-&gt;string"/>
        <StringDetect attribute="Special" context="#stay" String="string-&gt;list"/>
        <StringDetect attribute="Special" context="#stay" String="char-&gt;integer"/>
        <StringDetect attribute="Special" context="#stay" String="integer-&gt;char"/>
        <StringDetect attribute="Special" context="#stay" String="string-&gt;symbol"/>
