        <StringDetect attribute="Special" context="#stay" String="exact-&gt;inexact"/>
        <StringDetect attribute="Special" context="#stay" String="inexact-&gt;exact"/>
        <StringDetect attribute="Special" context="#stay" String="number-&gt;string"/>
        <StringDetect attribute="Special" context="#stay" String="string-&gt;number"/>
        <RegExpr attribute="Predicate" context="#stay" String="[&lt;&gt;]?=?\s"/>
        <DetectChar attribute="Comment" context="String" char="&quot;"/>
        <StringDetect attribute="Decimal" context="#stay" String="'()"/>
        <RegExpr attribute="Special" context="#stay" String="['`]"/>
        <RegExpr attribute="Char" context="SpecialNumber" String="#[bodxei]"/>
        <RegExpr attribute="Decimal" context="#stay" String="#[tf]"/>
        <Float attribute="Float" context="#stay"/>
        <Int attribute="Decimal" context="#stay"/>
      </context>
      <context name="Comment" attribute="Comment" lineEndContext="#pop"/>
      <context name="MultiLineComment" attribute="Comment" lineEndContext="#stay">
        <Detect2Chars attribute="Comment" context="#pop" char="#" char1="|"/>
      </context>
      <context name="Predicates" attribute="Normal" lineEndContext="#pop">
        <DetectChar attribute="Predicate" context="#pop" char="?"/>
      </context>
      <context name="Predicates2" attribute="Normal" lineEndContext="#pop">
        <RegExpr attribute="Predicate" context="#pop" String="[&lt;&gt;]?=?\?"/>
      </context>
      <context name="Assignments" attribute="Normal" lineEndContext="#pop">
        <DetectChar attribute="Assignments" context="#pop" char="!"/>
      </context>
      <context name="SpecialNumber" attribute="Normal" lineEndContext="#pop">
        <Float attribute="Float" context="#pop"/>
        <Int attribute="Decimal" context="#pop"/>
        <HlCOct attribute="BaseN" context="#pop"/>
        <HlCHex attribute="Float" context="#pop"/>
      </context>
      <context name="String" attribute="String" lineEndContext="#stay">
        <DetectChar attribute="Comment" context="#pop" char="&quot;"/>
      </context>
