<!DOCTYPE kpartgui>
<kpartgui name="KWritePart">
<MenuBar>
  <Menu name="Edit"><text>&amp;Edit</text>
   <Action name="edit_undo"/>
   <Action name="edit_redo"/>
   <Action name="undo_redo_history"/>
   <Separator/>
   <Action name="edit_cut"/>
   <Action name="edit_copy"/>
   <Action name="edit_paste"/>
   <Separator/>
   <Action name="find"/>
   <Action name="find_again"/>
   <Action name="replace"/>
   <Separator/>
   <Action name="select_all"/>
   <Action name="deselect_all"/>
   <Action name="invert_select"/>
   <Separator/>
   <Action name="goto_line"/>
  </Menu>
  <Menu name="tools"><text>&amp;Tools</text>
   <Action name="indent"/>
   <Action name="unindent"/>
   <Action name="clean_indent"/>
   <Separator/>
   <Action name="highlight_select"/>
   <Separator/>
   <Action name="spell_checking"/>
  </Menu>
  <Menu name="Bookmarks"><text>&amp;Bookmarks</text>
   <Action name="set_bookmark"/>
   <Action name="add_bookmark"/>
   <Action name="clear_bookmarks"/>
   <Separator/>
   <Action name="list_of_bookmarks"/>
  </Menu>
</MenuBar>
<ToolBar name="mainToolBar">
 <Action name="edit_cut"/>
 <Action name="edit_copy"/>
 <Action name="edit_paste"/>
 <Separator/>
 <Action name="edit_undo"/>
 <Action name="edit_redo"/>
 <Separator/>
 <Action name="file_print"/>
</ToolBar>
</kpartgui>
